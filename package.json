{
  "name": "bolaget.io",
  "version": "1.6.1",
  "description": "RESTful(ish) JSON API for Swedish alcohol monopoly, Systembolaget",
  "main": "app/web.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rm -rf .build && cp -rf app .build && babel app -d .build",
    "dev": "DEBUG=koa* nodemon app/web.js --exec babel-node",
    "dev:worker": "babel-node app/worker.js",
    "precommit": "lint-staged",
    "standard": "standard"
  },
  "author": "Fredrik Lack <fredrik@fre.la>",
  "url": "https://github.com/larsha/bolaget.io/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/larsha/bolaget.io"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "env": ["jest"]
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.39",
    "@babel/core": "7.0.0-beta.39",
    "@babel/node": "7.0.0-beta.39",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.39",
    "@babel/preset-env": "7.0.0-beta.39",
    "@babel/register": "7.0.0-beta.39",
    "jest": "22.2.2",
    "babel-jest": "22.2.2",
    "babel-core": "^7.0.0-0",
    "nock": "9.1.5",
    "nodemon": "1.14.12",
    "standard": "10.0.3",
    "lint-staged": "6.1.0",
    "husky": "0.14.3"
  },
  "dependencies": {
    "elasticsearch": "14.0.0",
    "es6-promise": "4.2.4",
    "handlebars": "4.0.11",
    "http-shutdown": "1.2.0",
    "koa": "2.4.1",
    "koa-logger": "3.1.0",
    "koa-router": "7.4.0",
    "koa-views": "5.0.2",
    "winston": "2.2.0",
    "xml2js": "0.4.19"
  }
}
