{
  "name": "bolaget.io",
  "version": "0.5.0",
  "description": "RESTful JSON API for Swedish alcohol monopoly, Systembolaget",
  "main": "app/web.js",
  "engines": {
    "node": ">=7.2.0"
  },
  "scripts": {
    "build": "rm -rf .build && cp -rf app .build && babel app -d .build",
    "start": "node --harmony-async-await .build/web.js",
    "worker": "npm run build && node --harmony-async-await .build/worker.js",
    "production": "npm run build && npm start",
    "dev": "DEBUG=koa* nodemon --harmony --debug app/web.js --exec babel-node",
    "dev:worker": "babel-node --harmony app/worker.js",
    "test": "mocha --harmony --compilers js:babel-core/register --colors app/lib/test/*.spec.js"
  },
  "author": "Fredrik Lack <fredrik@brynn.se>",
  "url": "https://github.com/larsha/bolaget.io",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "before",
      "after"
    ]
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.10.1",
    "babel-eslint": "6.0.5",
    "babel-preset-node7": "1.3.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "mocha": "3.1.0",
    "nodemon": "1.9.2",
    "standard": "7.1.2",
    "nock": "9.0.2"
  },
  "dependencies": {
    "elasticsearch": "11.0.1",
    "handlebars": "4.0.5",
    "koa": "2.0.0",
    "koa-bodyparser": "3.1.0",
    "koa-logger": "2.0.0",
    "koa-simple-router": "0.2.0",
    "koa-views": "5.0.2",
    "winston": "2.2.0",
    "xml2js": "0.4.16"
  }
}
