{
  "name": "bolaget.io",
  "version": "0.1.1",
  "description": "RESTful JSON API for Swedish alcohol monopoly, Systembolaget",
  "main": "app/web.js",
  "engines": {
    "node": "6.5.0"
  },
  "scripts": {
    "build": "rm -rf .build && cp -rf app .build && babel app -d .build",
    "start": "node .build/web.js",
    "worker": "npm run build && node .build/worker.js",
    "production": "npm run build && npm start",
    "dev": "DEBUG=koa* nodemon --debug app/web.js --exec babel-node",
    "elasticDebug": "ELASTIC_LOG=trace DEBUG=koa* nodemon --debug app/web.js --exec babel-node",
    "dev:worker": "babel-node --debug app/worker.js",
    "test": "ELASTIC_HOST=127.0.0.1:9200 mocha --compilers js:babel-register --recursive 'app'"
  },
  "author": "Fredrik Lack <fredrik@nody.se>",
  "url": "https://github.com/larsha/bolaget.io",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-eslint": "6.0.5",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "mocha": "3.1.0",
    "nodemon": "1.9.2",
    "standard": "7.1.2"
  },
  "dependencies": {
    "babel-cli": "6.10.1",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.8.0",
    "elasticsearch": "11.0.1",
    "handlebars": "4.0.5",
    "koa": "2.0.0",
    "koa-bodyparser": "3.1.0",
    "koa-logger": "2.0.0",
    "koa-simple-router": "0.2.0",
    "koa-views": "5.0.2",
    "winston": "2.2.0",
    "xml2js": "0.4.16"
  }
}
