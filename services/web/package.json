{
  "name": "bolaget.io",
  "version": "1.8.17",
  "description": "RESTful(ish) JSON API for Swedish alcohol monopoly, Systembolaget",
  "main": "src/web.js",
  "scripts": {
    "test": "NODE_ENV=test && jest --verbose",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "build": "rm -rf .build && cp -rf src .build && babel src -d .build",
    "dev": "babel-node src/web.js",
    "dev:watch": "nodemon src/web.js",
    "dev:worker": "babel-node src/worker.js"
  },
  "author": "Fredrik Lack <fredrik@sthlm.io>",
  "url": "https://github.com/larsha/bolaget.io/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/larsha/bolaget.io"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.build/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.12.3",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@babel/preset-env": "7.11.5",
    "babel-jest": "26.3.0",
    "jest": "26.4.2",
    "nock": "13.0.4",
    "nodemon": "2.0.4",
    "prettier": "2.1.2"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.5.1",
    "dotenv": "8.2.0",
    "handlebars": "4.7.6",
    "http-shutdown": "1.2.2",
    "koa": "2.13.0",
    "koa-logger": "3.2.1",
    "koa-router": "9.4.0",
    "koa-views": "6.3.1",
    "winston": "3.3.3",
    "xml2js": "0.4.23"
  }
}
