{
  "name": "bolaget.io",
  "version": "1.7.11",
  "description": "RESTful(ish) JSON API for Swedish alcohol monopoly, Systembolaget",
  "main": "src/web.js",
  "scripts": {
    "test": "NODE_ENV=test && jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "build": "rm -rf .build && cp -rf src .build && babel src -d .build",
    "dev": "babel-node src/web.js",
    "dev:watch": "nodemon src/web.js",
    "dev:worker": "babel-node src/worker.js"
  },
  "author": "Fredrik Lack <fredrik@sthlm.io>",
  "url": "https://github.com/larsha/bolaget.io/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/larsha/bolaget.io"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.build/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/node": "7.7.0",
    "@babel/plugin-proposal-optional-chaining": "7.7.4",
    "@babel/preset-env": "7.7.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "nock": "11.7.0",
    "nodemon": "2.0.0",
    "standard": "14.3.1"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.4.0",
    "dotenv": "8.2.0",
    "handlebars": "4.5.3",
    "http-shutdown": "1.2.1",
    "koa": "2.11.0",
    "koa-logger": "3.2.1",
    "koa-router": "7.4.0",
    "koa-views": "6.2.1",
    "winston": "3.2.1",
    "xml2js": "0.4.22"
  }
}
