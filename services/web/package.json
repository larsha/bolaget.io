{
  "name": "bolaget.io",
  "version": "1.7.0",
  "description": "RESTful(ish) JSON API for Swedish alcohol monopoly, Systembolaget",
  "main": "src/web.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rm -rf .build && cp -rf src .build && babel src -d .build",
    "dev": "DEBUG=koa* nodemon src/web.js --exec babel-node",
    "dev:worker": "babel-node src/worker.js",
    "precommit": "lint-staged",
    "standard": "standard"
  },
  "author": "Fredrik Lack <fredrik@fre.la>",
  "url": "https://github.com/larsha/bolaget.io/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/larsha/bolaget.io"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.build/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "husky": "2.4.0",
    "jest": "24.8.0",
    "lint-staged": "8.2.0",
    "nock": "10.0.6",
    "nodemon": "1.19.1",
    "standard": "12.0.1"
  },
  "dependencies": {
    "elasticsearch": "16.1.1",
    "es6-promise": "4.2.8",
    "handlebars": "4.1.2",
    "http-shutdown": "1.2.1",
    "koa": "2.7.0",
    "koa-logger": "3.2.0",
    "koa-router": "7.4.0",
    "koa-views": "6.2.0",
    "winston": "2.2.0",
    "xml2js": "0.4.19"
  }
}
