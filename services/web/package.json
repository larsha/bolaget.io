{
  "name": "bolaget.io",
  "version": "1.7.11",
  "description": "RESTful(ish) JSON API for Swedish alcohol monopoly, Systembolaget",
  "main": "src/web.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rm -rf .build && cp -rf src .build && babel src -d .build",
    "dev": "DEBUG=koa* nodemon src/web.js --exec babel-node",
    "dev:worker": "babel-node src/worker.js",
    "precommit": "lint-staged",
    "standard": "standard"
  },
  "author": "Fredrik Lack <fredrik@sthlm.io>",
  "url": "https://github.com/larsha/bolaget.io/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/larsha/bolaget.io"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.build/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/register": "7.6.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.4.0",
    "nock": "11.3.5",
    "nodemon": "1.19.2",
    "standard": "14.3.1"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.3.0",
    "es6-promise": "4.2.8",
    "handlebars": "4.2.1",
    "http-shutdown": "1.2.1",
    "koa": "2.8.1",
    "koa-logger": "3.2.1",
    "koa-router": "7.4.0",
    "koa-views": "6.2.1",
    "winston": "3.2.1",
    "xml2js": "0.4.22"
  }
}
